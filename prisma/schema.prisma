generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Psicologos {
  id            String   @id @default(uuid()) 
  credencial   String   @unique @db.VarChar(6)
  nome          String
  email         String
  senha         String
  especialidade String
  createdAt     DateTime @default(now())
}
